{
  "name": "@se-2/hardhat",
  "version": "0.0.1",
  "scripts": {
    "account": "hardhat run scripts/listAccount.ts",
    "chain": "hardhat node --network hardhat --no-deploy",
    "compile": "hardhat compile",
    "deploy": "hardhat deploy",
    "fork": "MAINNET_FORKING_ENABLED=true hardhat node --network hardhat --no-deploy",
    "generate": "hardhat run scripts/generateAccount.ts",
    "flatten": "hardhat flatten",
    "lint": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore ./*.ts ./deploy/**/*.ts ./scripts/**/*.ts ./test/**/*.ts",
    "lint-staged": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore",
    "format": "prettier --write ./*.ts ./deploy/**/*.ts ./scripts/**/*.ts ./test/**/*.ts",
    "test": "REPORT_GAS=true hardhat test --network hardhat",
    "verify": "hardhat etherscan-verify",
    "hardhat-verify": "hardhat verify",
    "test:local": "hardhat --network localhost test",
    "test:deploy": "hardhat --network hardhat deploy",
    "build": "rm -rf ./build/deploy ./build/hardhat.config.js && hardhat compile && tsc",
    "prepublishOnly": "yarn build",
    "pub": "yarn publish --access public",
    "prepare": "husky install",
    "wikiCheck": "node wikiCheck.js "
  },
  "files": [
    "build",
    "contracts/**/*.sol",
    "artifacts",
    "deployments/mainnet/*.json",
    "deployments/archive/**/*.json"
  ],
  "devDependencies": {
    "@ensdomains/dnsprovejs": "^0.3.7",
    "@ensdomains/test-utils": "^1.3.0",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.1",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.3",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-verify": "^2.0.3",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/test-helpers": "^0.5.11",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^9.1.0",
    "@types/eslint": "^8",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2",
    "@types/qrcode": "^1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "chai": "^4.3.6",
    "dotenv": "^10.0.0",
    "elliptic-solidity": "^1.0.0",
    "envfile": "^6.17.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^6.10.0",
    "hardhat": "^2.19.4",
    "hardhat-abi-exporter": "^2.9.0",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-deploy": "^0.11.45",
    "hardhat-deploy-ethers": "^0.4.1",
    "hardhat-gas-reporter": "^1.0.9",
    "husky": "^8.0.0",
    "prettier": "^2.8.4",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "rfc4648": "^1.5.0",
    "solidity-coverage": "^0.8.5",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@ensdomains/buffer": "^0.1.1",
    "@ensdomains/solsha1": "0.0.3",
    "@openzeppelin/contracts": "^4.8.1",
    "@typechain/ethers-v6": "^0.5.1",
    "dns-packet": "^5.3.0",
    "dotenv": "^16.0.3",
    "envfile": "^6.18.0",
    "qrcode": "^1.5.1"
  },
  "resolutions": {
    "ethereum-ens": "0.8.0",
    "js-sha3": "0.8.0"
  },
  "directories": {
    "test": "test"
  }
}
